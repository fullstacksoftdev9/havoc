{
    "name": "havoc-repo",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": ">=14.0.0"
    },
    "scripts": {
        "lint": "eslint --fix '*/**/*.{js,ts,tsx}' --quiet --ignore-path ./.gitignore",
        "lint-check": "eslint '*/**/*.{js,ts,tsx}' --max-warnings=0 --ignore-path ./.gitignore",
        "dev": "next dev -p 3000",
        "build": "rm -rf ./build && NODE_ENV=production next build",
        "start-local": "next start -p 3000",
        "start": "next start -p $PORT",
        "deploy": "yarn run build && gcloud app deploy --quiet --project havoc-repo",
        "prepare": "husky install"
    },
