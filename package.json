        "@emotion/react": "^11.1.5",
        "@emotion/styled": "^11.3.0",
        "@google-cloud/storage": "^5.8.2",
        "@google-cloud/tasks": "^2.3.0",
        "@typegoose/typegoose": "^7.6.0",
        "@types/mongoose": "5.10.x",
        "array-move": "^3.0.1",
        "busboy": "^0.3.1",
        "chakra-ui-markdown-renderer": "^1.1.0",
        "debian-control": "^1.0.0",
        "draft-js": "^0.11.7",
        "framer-motion": "^4.1.4",
        "hashids": "^2.2.8",
        "mongodb": "^3.6.5",
        "mongoose": "5.10.18",
        "next": "^10.1.3",
        "next-auth": "^3.14.6",
        "next-connect": "^0.10.1",
